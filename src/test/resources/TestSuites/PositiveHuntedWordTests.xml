<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="PositiveHuntedWordTestSuite" verbose="1" parallel="tests" thread-count="4">

    <test name="PositiveHunted1Word-CH">
        <parameter name="browser" value="chrome"/>
        <parameter name="word" value="welcome"/>
        <classes>
            <class name="ru.wooordhunt.huntedWordTest.PositiveHuntedWordTest"/>
        </classes>
    </test>

    <test name="PositiveHunted1Word-FF">
        <parameter name="browser" value="firefox"/>
        <parameter name="word" value="welcome"/>
        <classes>
            <class name="ru.wooordhunt.huntedWordTest.PositiveHuntedWordTest"/>
        </classes>
    </test>

    <test name="PositiveHunted2Word-CH">
        <parameter name="browser" value="chrome"/>
        <parameter name="word" value="incorrect"/>
        <classes>
            <class name="ru.wooordhunt.huntedWordTest.PositiveHuntedWordTest"/>
        </classes>
    </test>

    <test name="PositiveHunted2Word-FF">
        <parameter name="browser" value="firefox"/>
        <parameter name="word" value="incorrect"/>
        <classes>
            <class name="ru.wooordhunt.huntedWordTest.PositiveHuntedWordTest"/>
        </classes>
    </test>

</suite>